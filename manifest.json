{
    "manifest_version": 2,
    "name": "HTML Parser Chrome Extension",
    "version": "1.0",
    "description": "A simple extension to display parsed information from HTML.",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "run_at": "document_end"
        }
      
    ],
    "permissions": ["activeTab"]
  }
  